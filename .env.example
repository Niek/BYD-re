# BYD Client Configuration
# Copy this file to .env and fill in with your actual credentials
# .env is ignored by git and should never be committed

# Required: BYD account credentials
BYD_USERNAME=you@example.com
BYD_PASSWORD=your-password

# Optional: Device and regional configuration
BYD_COUNTRY_CODE=NL
BYD_LANGUAGE=en
BYD_IMEI_MD5=00000000000000000000000000000000
BYD_VIN=

# Optional: Network and device details
BYD_NETWORK_TYPE=wifi
BYD_APP_INNER_VERSION=220
BYD_APP_VERSION=2.2.1
BYD_OS_TYPE=15
BYD_OS_VERSION=35
BYD_TIME_ZONE=Europe/Amsterdam
BYD_DEVICE_TYPE=0
BYD_MOBILE_BRAND=XIAOMI
BYD_MOBILE_MODEL=POCO F1
BYD_SOFT_TYPE=0
BYD_TBOX_VERSION=3
BYD_IS_AUTO=1
BYD_OSTYPE=and
BYD_IMEI=BANGCLE01234
BYD_MAC=00:00:00:00:00:00
BYD_MODEL=POCO F1
BYD_SDK=35
BYD_MOD=Xiaomi

# Server configuration (when using server.js)
PORT=3000
REFRESH_INTERVAL_MINUTES=15
